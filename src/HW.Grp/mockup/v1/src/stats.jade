extends includes/layout
block content

  include includes/navbar.jade
  mixin navbar('stats.html', 'View results and compare groups')
  
  .contentgroup.grid_16
    #contextbar
      .settingsPane
        span.desc Timeframe
        span.input-daterange
          input.input-small.date(readonly='', style='cursor:pointer', type='text', name='startDate', value='2015 Dec')
          span --
          input.input-small.date(readonly='', style='cursor:pointer', type='text', name='endDate', value='2016 Dec')
        |  Survey 
        select
          option(selected='selected', value='SPRU2643') Health & Stress
        |  Aggregation 
        select
          option(value='1') One week
          option(selected='selected', value='7') Two weeks, start with even
          option(value='2') Two weeks, start with odd
          option(value='3') One month
          option(value='4') 3 months
          option(value='5') 6 months
          option(value='6') One year
        br
        span.desc Grouping 
        select
          option(selected='selected', value='0') <none >
          option(value='1') Users on unit
          option(value='2') Users on unit+subunits
          option(value='3') Background variable
        br
        input.btn(type='submit', value='Execute')
    .report-parts
      .action
        span.small Change all graphs to:
