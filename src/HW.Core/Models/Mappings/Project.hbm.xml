<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="HW.Core.Models" assembly="HW.Core">
	<class name="Project" table="Project">
		<id name="Id" column="ProjectID">
			<generator class="identity" />
		</id>
		<property name="Internal" />
		<bag name="Rounds" inverse="true" cascade="all-delete-orphan">
			<key column="ProjectID" />
			<one-to-many class="ProjectRound" />
		</bag>
	</class>
	<class name="ProjectRound" table="ProjectRound">
		<id name="Id" column="ProjectRoundID">
			<generator class="identity" />
		</id>
		<property name="Internal" />
		<property name="Started" />
		<many-to-one name="Project" class="Project" column="ProjectID" />
		<many-to-one name="Report" class="Report" column="ReportID" />
		<bag name="Languages" inverse="true" cascade="all-delete-orphan">
			<key column="ProjectRoundID" />
			<one-to-many class="ProjectRoundLanguage" />
		</bag>
		<bag name="Units" inverse="true" cascade="all-delete-orphan">
			<key column="ProjectRoundID" />
			<one-to-many class="ProjectRoundUnit" />
		</bag>
	</class>
	<class name="ProjectRoundLanguage" table="ProjectRoundLang">
		<id name="Id" column="ProjectRoundLangID">
			<generator class="identity" />
		</id>
		<many-to-one name="Round" class="ProjectRound" column="ProjectRoundID" />
		<many-to-one name="Language" class="Language" column="LangID" />
		<property name="InvitationSubject" />
	</class>
	<class name="ProjectRoundUnit" table="ProjectRoundUnit">
		<id name="Id" column="ProjectRoundUnitID">
			<generator class="identity" />
		</id>
		<property name="Name" column="Unit" />
		<many-to-one name="ProjectRound" class="ProjectRound" column="ProjectRoundID" />
		<many-to-one name="Report" class="Report" column="ReportID" />
		<many-to-one name="Language" class="Language" column="LangID" />
		<many-to-one name="Survey" class="Survey" column="SurveyID" />
	</class>
	<class name="ProjectRoundUser" table="ProjectRoundUser">
		<id name="Id" column="ProjectRoundUserID">
			<generator class="identity" />
		</id>
		<property name="Email" />
	</class>
</hibernate-mapping>