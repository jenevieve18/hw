<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="HW.Core.Models" assembly="HW.Core">
	<class name="Exercise" table="Exercise">
		<id name="Id" column="ExerciseID">
			<generator class="identity" />
		</id>
		<property name="Image" column="ExerciseImg" />
		<many-to-one name="Area" class="ExerciseArea" column="ExerciseAreaID" />
		<many-to-one name="Category" class="ExerciseCategory" column="ExerciseCategoryID" />
		<bag name="Variants" inverse="true" cascade="all-delete-orphan">
			<key column="ExerciseID" />
			<one-to-many class="ExerciseVariant" />
		</bag>
	</class>
	<class name="ExerciseArea" table="ExerciseArea">
		<id name="Id" column="ExerciseAreaID">
			<generator class="identity" />
		</id>
		<property name="Image" column="ExerciseAreaImg" />
	</class>
	<class name="ExerciseCategory" table="ExerciseCategory">
		<id name="Id" column="ExerciseCategoryID">
			<generator class="identity" />
		</id>
		<property name="SortOrder" column="ExerciseCategorySortOrder" />
	</class>
	<class name="ExerciseVariant" table="ExerciseVariant">
		<id name="Id" column="ExerciseVariantID">
			<generator class="identity" />
		</id>
		<many-to-one name="Exercise" class="Exercise" column="ExerciseID" />
		<many-to-one name="Type" class="ExerciseType" column="ExerciseTypeID" />
		<bag name="Languages" inverse="true" cascade="all-delete-orphan">
			<key column="ExerciseVariantID" />
			<one-to-many class="ExerciseVariantLanguage" />
		</bag>
	</class>
	<class name="ExerciseType" table="ExerciseType">
		<id name="Id" column="ExerciseTypeID">
			<generator class="identity" />
		</id>
		<property name="SortOrder" column="ExerciseTypeSortOrder"/>
	</class>
	<class name="ExerciseCategoryLanguage" table="ExerciseCategoryLang">
		<id name="Id" column="ExerciseCategoryLangID">
			<generator class="identity" />
		</id>
		<many-to-one name="Category" class="ExerciseCategory" column="ExerciseCategoryID" />
		<property name="CategoryName" column="ExerciseCategory" />
		<many-to-one name="Language" class="Language" column="Lang" />
	</class>
	<class name="ExerciseAreaLanguage" table="ExerciseAreaLang">
		<id name="Id" column="ExerciseAreaLangID">
			<generator class="identity" />
		</id>
		<many-to-one name="Area" class="ExerciseArea" column="ExerciseAreaID" />
		<property name="AreaName" column="ExerciseArea" />
		<many-to-one name="Language" class="Language" column="Lang" />
	</class>
	<class name="ExerciseVariantLanguage" table="ExerciseVariantLang">
		<id name="Id" column="ExerciseVariantLangID">
			<generator class="identity" />
		</id>
		<many-to-one name="Variant" class="ExerciseVariant" column="ExerciseVariantID" />
		<property name="File" column="ExerciseFile" />
		<property name="Size" column="ExerciseFileSize" />
		<property name="Content" column="ExerciseContent" />
		<property name="ExerciseWindowX" column="ExerciseWindowX" />
		<property name="ExerciseWindowY" column="ExerciseWindowY" />
		<many-to-one name="Language" class="Language" column="Lang" />
	</class>
	<class name="ExerciseTypeLanguage" table="ExerciseTypeLang">
		<id name="Id" column="ExerciseTypeLangID">
			<generator class="identity" />
		</id>
		<many-to-one name="Type" class="ExerciseType" column="ExerciseTypeID" />
		<property name="TypeName" column="ExerciseType" />
		<property name="SubTypeName" column="ExerciseSubtype"/>
		<many-to-one name="Language" class="Language" column="Lang" />
	</class>
</hibernate-mapping>