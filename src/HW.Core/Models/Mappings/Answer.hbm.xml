<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="HW.Core.Models" assembly="HW.Core">
	<class name="Answer" table="Answer">
		<id name="Id" column="AnswerID">
			<generator class="identity" />
		</id>
		<property name="StartDate" column="StartDT" />
		<property name="EndDate" column="EndDT" />
		<many-to-one name="ProjectRound" class="ProjectRound" column="ProjectRoundID" />
		<many-to-one name="ProjectRoundUnit" class="ProjectRoundUnit" column="ProjectRoundUnitID" />
		<many-to-one name="ProjectRoundUser" class="ProjectRoundUser" column="ProjectRoundUserID" />
	</class>
	
	<class name="BackgroundAnswer" table="BA">
		<id name="Id" column="BAID">
			<generator class="identity" />
		</id>
		<property name="Internal" />
		<many-to-one name="BackgroundQuestion" class="BackgroundQuestion" column="BQID" />
	</class>
</hibernate-mapping>